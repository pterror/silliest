---
import ComfyuiInfoComponent from "../../../slices/image-gen-tools/ComfyuiInfo.vue";
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/png" href="/3c.png" />
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <meta name="generator" content={Astro.generator} />
    <meta
      content="stable diffusion webui metadata viewer"
      property="og:site_name"
    />
    <meta
      content="view metadata for images generated by stable diffusion webui"
      property="og:description"
    />
    <link rel="shortcut icon" href="/3c.png" />
    <title>comfyui info</title>
    <link rel="stylesheet" href="/style.css" />
  </head>
  <body class="fullscreen-body">
    <ComfyuiInfoComponent client:only />
  </body>
</html>
