      content="it's like chub.ai but it looks different"
      property="og:description"
    />
    <link rel="shortcut icon" href="/3c.png" />
    <title>chub</title>
    <link rel="stylesheet" href="/style.css" />
  </head>
  <body class="fullscreen-body">
    <Navbar />
    <ChubStatsComponent client:only />
  </body>
</html>
